<?xml version="1.0"?>
<package format="2">
  <name>smile_mobile_hardware</name>
  <version>0.0.0</version>
  <description>
    This package contains contents necessary for smile mobile robots physical hardware.
  </description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
